<Window x:Class="VictorAct0.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VictorAct0"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content = " Up left" Click="Button_Click" Margin="0,0,675,408" Foreground="#FF6A8C14"></Button>
        <Button Content = " Up right" Click="Button_Click" Margin="675,0,0,408" Background="#FFF5F3F3" Foreground="Red"></Button>
